production:
  adapter:  mysql2
  host:     {{ openproject_mysql.host }}
  database: {{ openproject_mysql.name }}
  username: {{ openproject_mysql.user }}
  password: {{ openproject_mysql.pass }}
  encoding: utf8

development:
  adapter:  mysql2
  host:     localhost
  database: openproject_dev
  username: openproject
  password: openproject
  encoding: utf8

# Warning: The database defined as "test" will be erased and
